kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: dotnet-iis-app
  namespace: demo-vm-ossm
spec:
  to:
    kind: Service
    name: dotnet-iis-app
    weight: 100
  port:
    targetPort: http
  tls:
    termination: edge
    insecureEdgeTerminationPolicy: Redirect
  wildcardPolicy: None